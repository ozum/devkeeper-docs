{
  "name": "devkeeper-docs",
  "description": "README and VuePress generator",
  "version": "0.1.1",
  "author": "Özüm Eldoğan",
  "keywords": [
    "readme",
    "vuepress",
    "devkeeper"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "devkeeper-docs": "./bin/run"
  },
  "license": "MIT",
  "engines": {
    "node": ">=11"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ozum/devkeeper-docs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ozum/devkeeper-docs.git"
  },
  "bugs": {
    "url": "https://github.com/ozum/devkeeper-docs/issues"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && bin/run readme && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "bin/run readme && oclif-dev readme && git add README.md",
    "release": "git push --follow-tags origin master && git push --tags && npm publish"
  },
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3.0.0",
    "concat-md": "^0.3.4",
    "execa": "^4.0.1",
    "fs-extra": "^9.0.0",
    "readmeasy": "^0.1.7",
    "tslib": "^1.11.2",
    "typedoc": "^0.17.6",
    "typedoc-neo-theme": "^1.0.8",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-external-module-map": "^1.2.1",
    "typedoc-plugin-external-module-name": "^3.1.0",
    "typedoc-plugin-markdown": "^2.2.17",
    "typedoc-plugin-npm-externals": "^1.0.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/fs-extra": "^8.1.0",
    "@types/node": "^10.17.21",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.1"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "devkeeper-docs",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  }
}
